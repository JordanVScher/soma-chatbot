{
  "name": "soma-chatbot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "test": "jest --detectOpenHandles",
    "pm2_add": "pm2 start app/index.js  --name \"soma-homol\"",
    "restart": "pm2 restart \"soma-homol\" --update-env",
    "logs": "pm2 logs \"soma-homol\" --lines 1000",
    "stop": "pm2 stop \"soma-homol\"",
    "remove": "pm2 remove \"soma-homol\"",
    "dpm2_add": "pm2 start app/index.js  --name \"soma-dev\"",
    "drestart": "pm2 restart \"soma-dev\" --update-env",
    "dlogs": "pm2 logs \"soma-dev\" --lines 1000",
    "dstop": "pm2 stop \"soma-dev\"",
    "dremove": "pm2 remove \"soma-dev\"",
    "webhook": "npx bottender messenger webhook set",
    "profileGet": "npx bottender messenger profile get",
    "profileSet": "npx bottender messenger profile set"
  },
  "dependencies": {
    "@sentry/node": "^5.15.4",
    "apiai-promise": "^1.0.0",
    "async-child-process": "^1.1.1",
    "body-parser": "^1.19.0",
    "bottender": "^1.3.5",
    "child_process": "^1.0.2",
    "dialogflow": "^1.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "fs-extra": "^7.0.0",
    "get-audio-duration": "0.0.1",
    "grpc": "^1.24.2",
    "itsa-surveymonkey-promise": "0.0.6",
    "jest": "^25.3.0",
    "messaging-api-messenger": "^0.7.16",
    "moment": "^2.24.0",
    "nodemon": "^2.0.2",
    "pg": "^7.14.0",
    "promisepipe": "^3.0.0",
    "query-string": "^5.1.0",
    "remove-accents": "^0.4.2",
    "request": "^2.88.0",
    "requisition": "^1.7.0",
    "restify": "^8.5.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "surveymonkey": "^1.1.0",
    "util": "^0.12.0",
    "util.promisify": "^1.0.0",
    "validar-cpf": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
