{
  "name": "soma-chatbot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles",
    "app:build": "docker-compose up -d --build app",
    "app:log": "docker-compose logs app",
    "pm2_add": "pm2 start app/index.js  --name \"soma-homol\"",
    "restart": "pm2 restart \"soma-homol\" --update-env",
    "logs": "pm2 logs \"soma-homol\" --lines 1000",
    "stop": "pm2 stop \"soma-homol\"",
    "remove": "pm2 remove \"soma-homol\"",
    "dpm2_add": "pm2 start app/index.js  --name \"soma-dev\"",
    "drestart": "pm2 restart \"soma-dev\" --update-env",
    "dlogs": "pm2 logs \"soma-dev\" --lines 1000",
    "dstop": "pm2 stop \"soma-dev\"",
    "dremove": "pm2 remove \"soma-dev\"",
    "webhook": "npx bottender messenger webhook set",
    "profileGet": "npx bottender messenger profile get",
    "profileSet": "npx bottender messenger profile set",
    "bottender:dev": "npx bottender dev"
  },
  "dependencies": {
    "@sentry/node": "^5.21.1",
    "apiai-promise": "^1.0.0",
    "async-child-process": "^1.1.1",
    "body-parser": "^1.19.0",
    "bottender": "^1.4.10",
    "child_process": "^1.0.2",
    "dialogflow": "^1.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "fs-extra": "^7.0.0",
    "get-audio-duration": "0.0.1",
    "grpc": "^1.24.3",
    "itsa-surveymonkey-promise": "0.0.6",
    "jest": "^25.5.4",
    "messaging-api-messenger": "^0.7.18",
    "moment": "^2.27.0",
    "nodemon": "^2.0.2",
    "pg": "^7.18.2",
    "promisepipe": "^3.0.0",
    "query-string": "^5.1.0",
    "remove-accents": "^0.4.2",
    "request": "^2.88.2",
    "requisition": "^1.7.0",
    "restify": "^8.5.1",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "surveymonkey": "^1.1.0",
    "util": "^0.12.3",
    "util.promisify": "^1.0.1",
    "validar-cpf": "^2.1.2"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.22.0"
  }
}
